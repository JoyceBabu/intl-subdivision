{
    "name": "trickeyone/intl-subdivisions",
    "description": "A PHP Intl library to retrieve states for a given country.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh England",
            "email": "joshua.en@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/intl": "^3.1"
    },
    "suggest": {
        "ext-intl": "to use the component with locales other than \"en\""
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Component\\IntlSubdivisions\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "mockery/mockery": "^0.9.5"
    }
}
